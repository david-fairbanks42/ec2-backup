{
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.308.0",
        "guzzlehttp/guzzle": "^7.2",
        "nesbot/carbon": "3.8.4.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/functions.php"
        ]
    }
}
